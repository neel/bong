<?php
class Resource{
	public static function css($cssName){
		return MemPool::instance()->get("bong.url.root")."/rc/css/$cssName";
	}
	public static function js($jsName){
		var_dump(MemPool::instance()->get("bong.url.root"));
		return MemPool::instance()->get("bong.url.root")."/rc/js/$jsName";
	}
	public static function image($imageName){
		return MemPool::instance()->get("bong.url.root")."/rc/img/$imageName";
	}
}
